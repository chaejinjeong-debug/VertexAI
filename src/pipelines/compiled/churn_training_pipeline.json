          "cachingOptions": {
            "enableCache": true
          },
          "componentRef": {
            "name": "comp-model-upload-op"
          },
          "dependentTasks": [
            "eval-op",
            "train-op"
          ],
          "inputs": {
            "artifacts": {
              "input_metrics": {
                "taskOutputArtifact": {
                  "outputArtifactKey": "output_metrics",
                  "producerTask": "eval-op"
                }
              },
              "input_model": {
                "taskOutputArtifact": {
                  "outputArtifactKey": "output_model",
                  "producerTask": "train-op"
                }
